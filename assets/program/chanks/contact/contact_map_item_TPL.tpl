<div class="office">
    <div class="office-inner">
        <span class="marker-info">[[+coordLat]]&[[+coordLng]]&assets/images/contact-icon/marker-office[[+idx]].png</span>
        <div>
            <h5>[[+title]]</h5>
        </div>
        <div>
            <p>[[+address]]</p>
        </div>
        <div class="worktime">
            <p>[[%sheduble? &namespace=`unna`]]:</p>
            <p>
                  <span>
                    [[%work_schedule]]: [[+work_schedule]]
                  </span>
                <span>
                    [[%weekend_schedule]]: [[+weekend_schedule]]
                  </span>
            </p>
        </div>
        <div class="contact-info">
            <p><i class="icon-phone"></i> <a href="tel:+38 044 570 71 14"></a>[[+phone]]</p>
            <p><i class="icon-email"></i> <a href="mail:info@unna.com.ua">[[+email]]</a></p>
        </div>
        <div class="buttons">
            <a href="#" class="driving-direction">[[%shema_transfer]] <i class="icon-angle-right"></i></a>
            <a href="#" class="get-direction">[[%make_marshrut]] <i class="icon-angle-right"></i></a>
        </div>
    </div>
</div>
