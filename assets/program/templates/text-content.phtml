`]]

[[*template:ne=`36`:then=`
<script src="assets/js/jquery.mCustomScrollbar.concat.min.js"></script>
`:else=`
<script>
    var wrap = document.getElementsByClassName('wrapper')[0];

    var h1H = wrap.querySelector('h1').getBoundingClientRect().bottom;
    var footerH = wrap.querySelector('footer').getBoundingClientRect().top;

    var playerH = Math.round((footerH - h1H) * .8);
    var playerW = wrap.querySelector('.content').clientWidth * .7;

    wrap.querySelector('.content-wrap').style.textAlign = 'center';
    wrap.querySelector('.content-text').style.cssText = 'height: ' + (footerH - h1H) + 'px;padding-top: 5%;';
    wrap.querySelector('h1').style.cssText = 'width: ' + (playerW) + 'px;display: inline-block; text-align: left';

    // 2. This code loads the IFrame Player API code asynchronously.
    var tag = document.createElement('script');

    tag.src = "https://www.youtube.com/iframe_api";
    var firstScriptTag = document.getElementsByTagName('script')[0];
    firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);

    // 3. This function creates an <iframe> (and YouTube player)
    //    after the API code downloads.
    var player;
    function onYouTubeIframeAPIReady() {
        player = new YT.Player('playerYT', {
            height: playerH,
            width: playerW,
            videoId: 'M7lc1UVf-VE',
            events: {
                'onReady': onPlayerReady
            }
        });
    }

    // 4. The API will call this function when the video player is ready.
    function onPlayerReady(event) {
        event.target.playVideo();
    }
</script>
`]]
