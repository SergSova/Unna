<!doctype html>
<html lang="[[++cultureKey]]">
<head>
    <meta charset="UTF-8">
    <base href="[[++site_url]]">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>[[*pagetitle:striptags]]</title>
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400|Roboto:100,300,400,700&amp;subset=cyrillic"
          rel="stylesheet">
    <link rel="stylesheet" href="assets/css/jquery.mCustomScrollbar.min.css"/>

    [[*id:in=`[[babelId?&ids=`5,6,9,8,10,12,29,36,30`]]`:then=``:else=`
    <link rel="stylesheet" href="assets/css/complex.css">
    `]]
    [[*id:is=`[[babelId?&id=`9`]]`:then=`
    <link rel="stylesheet" href="assets/css/advantages.css">
    `]]
    [[*id:is=`[[babelId?&id=`30`]]`:then=`
    <link rel="stylesheet" href="assets/css/developer.css">
    `]]
    [[*id:is=`[[babelId?&id=`8`]]`:then=`
    <link rel="stylesheet" href="assets/css/location.css">
    `]]
    [[*id:is=`[[babelId?&id=`6`]]`:then=`
    <link rel="stylesheet" href="assets/css/contacts.css">
    `]]
    [[*id:is=`[[babelId?&id=`36`]]`:then=`
    <link rel="stylesheet" href="assets/css/actions.css">
    `]]
    [[*id:is=`[[babelId?&id=`12`]]`:then=`
    <link rel="stylesheet" href="assets/css/news.css">
    `]]
    [[*id:in=`[[babelId?&ids=`29,10`]]`:then=`
    <link rel="stylesheet" href="assets/css/masplan.css">
    `]]
    [[*id:is=`[[babelId?&id=`5`]]`:then=`
    <link rel="stylesheet" href="assets/css/documentation.css">
    `]]

    [[$seo.tpl]]
    <link rel="stylesheet" href="assets/css/queryLoader.css">
    [[*id:in=`[[babelId?&ids=`6,8`]]`:then=`
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCy85hurU5pPvup9lMcp2H6y5G3LrrsEXE" async defer></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
    `:else=`
    <script src="https://code.jquery.com/jquery-1.12.4.min.js"
            integrity="sha256-ZosEbRLbNQzLpnKIkEdrPv7lOy9C27hHQ+Xp8a4MxAQ="
            crossorigin="anonymous">
    </script>
    `]]
    <script src="assets/js/lib/queryLoader.js"></script>
</head>
<body>
[[$queryLoader]]
<!---->
<div class="wrapper" [[*katalog_image_bg:!empty=`style="background-image: url([[*katalog_image_bg]])"`]]>
    [[*id:is=`[[babelId?&id=`12`]]`:then=`<div class="to-blur" style="background-image: url('assets/images/news_bg.jpg')"></div>`]]
    <!--left side-->
    [[$left-side]]

    <!--right side-->
    [[$right-side]]

    <!--arrows-->
    [[!prev-next-Page]]

    [[*id:is=`[[babelId?&id=`6`]]`:then=`[[$contact_chank]]`:else=`
    <!--content-->
    <div class="content">
        <div class="content-wrap">
            <h1>[[#[[*parent]].pagetitle]]</h1>
            <h2>[[*pagetitle]]</h2>
            <div class="content-text[[*id:is=`[[babelId?&id=`8`]]`:then=` scale-js-1`]]">
                [[*id:is=`[[babelId?&id=`9`]]`:then=`[[$advantages]]`]]
                [[*id:is=`[[babelId?&id=`5`]]`:then=`[[$documents]]`]]
                [[*id:is=`[[babelId?&id=`8`]]`:then=`[[$location-content]]`]]
                [[*id:in=`[[babelId?&ids=`36,12`]]`:then=`[[$stocks]]`]]
                [[*id:is=`[[babelId?&id=`29`]]`:then=`<img src="[[*genplan]]" alt="Unna genplan">`]]
                [[*id:is=`[[babelId?&id=`30`]]`:then=`[[$developer_chank]]`]]

                <p>[[*content]]</p>
            </div>
        </div>
    </div>
    `]]
    [[*id:is=`[[babelId?&id=`9`]]`:then=`[[$advantages-slider]]`]]
    [[*id:is=`[[babelId?&id=`8`]]`:then=`[[$location-map-info]]`]]

    [[$foot]]
</div>

<!--scripts-->
<!--libs-->
<script type='text/javascript'>
	QueryLoader.init();
</script>
[[*id:in=`[[babelId?&id=`6,8`]]`:then=`
<script src="http://code.jquery.com/jquery-migrate-3.0.0.js"></script>
`:else=`

`]]
[[*id:is=`[[babelId?&id=`8`]]`:then=`
<script src="assets/js/location.js"></script>
`]]
[[*id:is=`[[babelId?&id=`6`]]`:then=`
<script src="assets/js/contacts.js"></script>
`]]

<script src="assets/js/jquery.mCustomScrollbar.concat.min.js"></script>
<!---->
<script src="assets/js/nav.js"></script>
<script src="assets/js/scale.js"></script>
[[!*id:ne=`[[babelId?&id=`6`]]`:and:ne=`[[babelId?&id=`12`]]`:then=`
<script src="assets/js/foot-menu.js"></script>
`]]
<script src="assets/js/start-anim.js"></script>
[[*id:in=`[[babelId?&ids=`6,9,8,12,36`]]`:then=``:else=`
<script src="assets/js/complex.js"></script>
`]]
[[*id:is=`[[babelId?&id=`9`]]`:then=`
<script src="assets/js/advantages.js"></script>
`]]
[[*id:is=`[[babelId?&id=`36`]]`:then=`
<script src="assets/js/actions.js"></script>
`]]
[[*id:is=`[[babelId?&id=`12`]]`:then=`
<script src="assets/js/news.js"></script>
<div id="fb-root"></div>
<script>
    (function (d, s, id) {
        var js, fjs = d.getElementsByTagName(s)[0];
        if (d.getElementById(id)) return;
        js = d.createElement(s);
        js.id = id;
        js.src = "//connect.facebook.net/ru_RU/sdk.js#xfbml=1&version=v2.9";
        fjs.parentNode.insertBefore(js, fjs);
    }(document, 'script', 'facebook-jssdk'));
</script>
`]]

</body>
</html>
